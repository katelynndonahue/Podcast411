{{!-- Cards for search --}}
  <div class="row">
    <div class="col s12 m7">
      <div class="card">
        <div class="card-image">
          <img src="{{thumbnail}}">
          <span class="card-title">{{title_highlighted}}</span>
        </div>
        <div class="card-content">
          <p>{{publisher_highlighted}}</p>
        </div>
        <div class="card-action">
          <a href="{{listennotes_url}}">Link to podcast!</a>
        </div>
      </div>
    </div>
  </div>