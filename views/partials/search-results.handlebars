{{!-- Cards for search --}}
  <div class="row search-row">
    <div class="col s12 m7">
      <div class="card">
        <div class="card-image">
          <img src="{{thumbnail}}">
        </div>
        <div class="card-content search-content">
          <p class="card-title search-title">{{title_highlighted}}</p>
          <p>{{publisher_highlighted}}</p>
        </div>
        <div class="card-action">
          <a href="{{listennotes_url}}">Link to podcast!</a>
          <button href="#" class="black-text saveToProfile">SAVE</button>

        </div>
      </div>
    </div>
  </div>